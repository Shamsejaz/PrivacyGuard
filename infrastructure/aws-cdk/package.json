{
  "name": "privacyguard-aws-cdk",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for PrivacyGuard AI Agent",
  "bin": {
    "privacyguard": "bin/privacyguard.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy --context environment=dev --require-approval never",
    "deploy:staging": "cdk deploy --context environment=staging --require-approval never",
    "deploy:prod": "cdk deploy --context environment=prod --require-approval broadening",
    "deploy:multi-region": "cdk deploy --all --context environment=prod --context enableMultiRegion=true --require-approval broadening",
    "destroy:dev": "cdk destroy --context environment=dev --force",
    "destroy:staging": "cdk destroy --context environment=staging --force",
    "diff:dev": "cdk diff --context environment=dev",
    "diff:staging": "cdk diff --context environment=staging",
    "diff:prod": "cdk diff --context environment=prod",
    "synth": "cdk synth",
    "synth:dev": "cdk synth --context environment=dev",
    "synth:staging": "cdk synth --context environment=staging",
    "synth:prod": "cdk synth --context environment=prod",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "aws-cdk": "2.95.1",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.95.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  },
  "keywords": [
    "aws",
    "cdk",
    "privacy",
    "compliance",
    "gdpr",
    "ccpa",
    "hipaa",
    "pdpl",
    "ai",
    "bedrock",
    "lambda",
    "serverless"
  ],
  "author": "PrivacyGuard Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/privacyguard/aws-agent.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}