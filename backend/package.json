{
  "name": "privacyguard-backend",
  "version": "1.0.0",
  "description": "Enterprise-grade backend API server for PrivacyGuard privacy compliance platform with comprehensive GDPR, CCPA, and PDPL compliance capabilities",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server-simple.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate": "tsx src/database/migrate.ts",
    "migrate:status": "tsx src/database/migrate.ts --status",
    "seed": "tsx src/database/seed.ts",
    "seed:users": "tsx src/database/seedUsers.ts",
    "seed:gdpr": "tsx src/database/seedGDPR.ts",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "health-check": "curl -f http://localhost:3001/health || exit 1",
    "docker:build": "docker build -t privacyguard-backend .",
    "docker:run": "docker run -p 3001:3001 privacyguard-backend"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.6.5",
    "pg": "^8.11.3",
    "redis": "^4.6.12",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@vitest/ui": "^1.1.0",
    "supertest": "^7.1.4",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "privacy",
    "compliance",
    "gdpr",
    "ccpa",
    "pdpl",
    "dsar",
    "risk-assessment",
    "policy-management",
    "data-protection",
    "privacy-ops",
    "typescript",
    "express",
    "postgresql",
    "mongodb",
    "redis"
  ],
  "author": "PrivacyGuard Team",
  "license": "PROPRIETARY",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/privacyguard.git",
    "directory": "backend"
  },
  "bugs": {
    "url": "https://github.com/your-org/privacyguard/issues"
  },
  "homepage": "https://github.com/your-org/privacyguard#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
